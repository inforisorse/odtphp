{
    "name": "inforisorse/odtphp",
    "description": "ODT PHP templating system",
    "version": "1.0.0",
    "keywords": [
        "odt",
        "php"
    ],
    "license": "GPL-3.0",
    "type": "library",
    "homepage": "https://github.com/inforisorse/odtphp",
    "require": {
        "php": ">=5.2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1",
        "symfony/var-dumper": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Odtphp\\": "src"
        },
        "files": ["lib/pclzip.lib.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Odtphp\\Test\\": "tests"
        },
        "files": ["lib/pclzip.lib.php"]
    }
}
